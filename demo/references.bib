	keywords = {Antwortverhalten, content analysis, data capture, data quality, Datengewinnung, Datenqualität, Fragebogen, Inhaltsanalyse, intercultural comparison, interkultureller Vergleich, online survey, Online-Befragung, pretest, Pretest, questionnaire, response behavior, survey research, Umfrageforschung, Validität, validity},
	file = {Behr et al. - Web probing - implementing probing techniques from.pdf:/Users/leaverou/Zotero/storage/JZ3CVIF3/Behr et al. - Web probing - implementing probing techniques from.pdf:application/pdf;Snapshot:/Users/leaverou/Zotero/storage/GNMD4QS6/56166.html:text/html},
